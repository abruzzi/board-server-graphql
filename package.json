{
  "name": "as4-generating-types-example",
  "version": "1.0.0",
  "description": "An example of generating types from a schema to use in your resolvers.",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "generate": "graphql-codegen --config codegen.yml",
    "compile": "npm run generate && tsc",
    "postinstall": "npm run compile",
    "start": "npm run compile && node ./dist/index.js",
    "deploy": "gcloud run deploy --source . --quiet",
    "watch": "concurrently  --names \"CodeGen,TS,Nodemon\" -c \"bgYellow,bgBlue,bgMagenta\" \"graphql-codegen --watch 'src/**/*.ts'\"  \"tsc -w \" \"nodemon ./dist/index.js \""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.10.4",
    "@graphql-codegen/cli": "^2.16.5",
    "@graphql-codegen/typescript": "^2.8.8",
    "@graphql-codegen/typescript-resolvers": "^2.7.13",
    "@prisma/client": "^5.16.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "google-auth-library": "^9.11.0",
    "graphql": "^16.9.0",
    "graphql-tag": "^2.12.6",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.14",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "typescript": "^4.8.3",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^18.7.18",
    "@types/nodemailer": "^6.4.15",
    "@types/uuid": "^10.0.0",
    "concurrently": "7.6.0",
    "nodemon": "2.0.22",
    "prisma": "^5.16.2"
  }
}
